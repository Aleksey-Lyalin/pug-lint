{
  "name": "jade-lint",
  "version": "2.0.0",
  "description": "An unopinionated and configurable linter and style checker for Jade",
  "author": "Ben Edwards <hello@benedfit.com>",
  "license": "ISC",
  "keywords": [
    "jadelint",
    "jade-lint",
    "jade",
    "lint",
    "code style",
    "formatter",
    "style guide",
    "validate",
    "lint jade",
    "jade lint",
    "linter",
    "linter jade",
    "jade linter",
    "jade-linter",
    "linting",
    "linting jade",
    "jade linting",
    "jade-linting",
    "hint",
    "hint jade",
    "jade hint",
    "jade-hint",
    "hinter",
    "hinter jade",
    "jade hinter",
    "jade-hinter",
    "hinting",
    "hinting jade",
    "jade hinting",
    "jade-hinting"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:benedfit/jade-lint.git"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "main": "./lib/linter",
  "bin": {
    "jade-lint": "./bin/jade-lint"
  },
  "scripts": {
    "lint": "eslint -f unix .",
    "inspect": "jsinspect",
    "coverage": "istanbul cover ./node_modules/.bin/_mocha test",
    "wrap": "rm -f npm-shrinkwrap.json; npm prune && npm install && npm shrinkwrap && pliers cleanShrinkwrap",
    "docs": "pliers buildRuleDocs",
    "pretest": "npm run docs && npm run lint && npm run inspect",
    "test": "npm run coverage",
    "posttest": "(istanbul check-coverage --statements 90 --branches 90 --functions 100 --lines 90 && rm -rf coverage) || echo Look at 'coverage/lcov-report/index.html' to find out more"
  },
  "dependencies": {
    "commander": "^2.9.0",
    "css-selector-parser": "^1.1.0",
    "glob": "^5.0.15",
    "jade-error": "^1.1.1",
    "jade-lexer": "jadejs/jade-lexer",
    "jsonminify": "^0.2.3",
    "minimatch": "^3.0.0",
    "void-elements": "^2.0.1"
  },
  "devDependencies": {
    "docco": "^0.7.0",
    "eslint": "^1.8.0",
    "eslint-config-clock": "^1.0.0",
    "eslint-config-standard": "^4.3.2",
    "eslint-plugin-standard": "^1.3.0",
    "istanbul": "^0.4.0",
    "jsinspect": "^0.7.0",
    "mocha": "^2.3.3",
    "pliers": "^1.2.1",
    "pliers-clean-shrinkwrap": "^1.0.1",
    "sinon": "^1.17.2"
  }
}
